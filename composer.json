{
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "sw-simple-slider",
                "version": "0.2.0",
                "source": {
                    "url": "git@github.com:swappsco/sw-simple-slider.git",
                    "type": "git",
                    "reference": "0.2.0"
                }
            }

        },
        {
            "type": "package",
            "package": {
                "name": "wpaasp-customizer",
                "version": "0.1.0",
                "source": {
                    "url": "git@github.com:swappsco/wpaasp-customizer.git",
                    "type": "git",
                    "reference": "0.1.0"
                }
            }
        }
            
    ],
    "require": {
        "vlucas/phpdotenv": "2.0.*",
        "wpackagist-plugin/addthis": "5.3.4",
        "wpackagist-plugin/amazon-web-services": "^1.0",
        "wpackagist-plugin/amazon-s3-and-cloudfront": "^1.1",
        "wpackagist-plugin/advanced-custom-fields-font-awesome": "1.7.3",
        "wpackagist-plugin/acf-image-crop-add-on": "1.4.10",
        "wpackagist-plugin/woocommerce": "2.6.13",
        "wpackagist-plugin/woocommerce-dropdown-cart": "2.1.1",
        "wpackagist-plugin/woocommerce-menu-bar-cart": "2.5.8.1",
        "wpackagist-plugin/regenerate-thumbnails": "2.2.6",
        "wpackagist-plugin/smtp-mailer": "1.0.2",
        "wpackagist-plugin/woocommerce-payu-latam-gateway": "1.2.3",
        "sw-simple-slider": "0.2.0",
        "wpaasp-customizer": "0.1.0",
        "oomphinc/composer-installers-extender": "^1.1"

    },
    "extra" : {
        "installer-types": ["library"],
        "installer-paths": {
            "wp-content/mu-plugins/{$name}/": [
            "sw-simple-slider",
            "wpaasp-customizer",
            "wpackagist-plugin/amazon-web-services",
            "wpackagist-plugin/amazon-s3-and-cloudfront",
            "wpackagist-plugin/advanced-custom-fields-font-awesome",
            "wpackagist-plugin/acf-image-crop-add-on"
            ]
        }
    }
}
