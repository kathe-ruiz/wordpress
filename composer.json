            "wpackagist-plugin/amazon-web-services",
            "wpackagist-plugin/amazon-s3-and-cloudfront",
            "wpackagist-plugin/mailgun",
            "wpackagist-plugin/acf-link-picker-field",
            "wpackagist-plugin/advanced-custom-fields-font-awesome",
